#!/usr/bin/env node
var letMeIn = require('../index.js');
var options = require('../lib/options');

var opts = options.parse()

if (opts) {
  var opts = {
    action: opts._[0],
    username: opts.u,
    password: opts.p,
    url: opts.step1,
    homePage: opts.step2,
    tokenPage: opts.step3,
    qty: opts.q || 1,
    stdout: true
  };

  letMeIn.getCodes(opts);
}
